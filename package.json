{
  "name": "@openclaw/browsy",
  "version": "0.1.0",
  "description": "Zero-render browser plugin for OpenClaw â€” fast, lightweight browsing for AI agents via browsy",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    }
  },
  "files": [
    "dist",
    "skills"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=22"
  },
  "peerDependencies": {
    "@openclaw/sdk": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@openclaw/sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@vitest/coverage-v8": "^3.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "openclaw",
    "browsy",
    "browser",
    "agent",
    "zero-render"
  ],
  "license": "MIT"
}
